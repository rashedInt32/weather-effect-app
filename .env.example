WEATHER_API_KEY=your_api_key_here
WEATHER_API_BASE_URL=https://api.openweathermap.org/data/2.5
WEATHER_API_TIMEOUT_MS=5000
WEATHER_API_MAX_REQUESTS_PER_MIN=60

CACHE_ENABLED=true
CACHE_TTL_SECONDS=300
